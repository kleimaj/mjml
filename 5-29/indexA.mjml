                </mj-column>
              <mj-column width="13%" css-class="icon-fix" vertical-align="middle" css-class="check-fix">
                    <mj-image alt="youtube logo" href="https://www.youtube.com/channel/UC5UMuK78o1sqOQdn1tugaIA" padding="0px 10px 0px 0px" align="right" width="38px" height="38px" alt="youtube" src="http://sendy.yourcreditninja.com/sendy/uploads/1685434121.png"></mj-image>
                </mj-column>
                <mj-column width="13%" css-class="icon-fix" vertical-align="middle" css-class="check-fix">
                    <mj-image padding="0px 0px 0px 0px" align="right" width="50px" height="50px" href="https://www.plaingreenloans.com/resources/about-us/" alt="logo" src="https://nm.my605lending.com/assets/footer-logo.png"></mj-image>
                </mj-column>
