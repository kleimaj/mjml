                padding: 0 30px 0 30px;
            }
            .name-block p br {
                display : none;
            }
            .name-block tr td {
               padding: 0px 0px 0px 0px !important;
            }
            .fname-block {
                text-align:center;
                padding: 0 !important;
            }
            
            .main-block {
                padding: 0 10px;
            }
            .name {
                font-size: 25px !important;
            }
            .footer-block {
                padding:  0 15px !important;
            }
            .advertisment p {
                text-align: center !important;
            }
            .check-fix  {
                width: 10% !important;
            }
            .copyright-fix {
                width: 100% !important;
            }
            .icon-fix {
